{
  "version": "0.1",
  "prefixes": {
    "skos": "http://www.w3.org/2004/02/skos/core#"
  },
  "resources": {
    "concept-schemes": {
      "class": "skos:ConceptScheme",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "features": [
        "include-uri"
      ],
     "new-resource-base": "http://data.lblod.info/id/concept-scheme/"
    },
    "concepts": {
      "class": "skos:Concept",
      "attributes": {
        "label": {
          "type": "string",
          "predicate": "skos:prefLabel"
        }
      },
      "relationships": {
        "in-scheme": {
          "target": "concept-schemes",
          "predicate": "skos:inScheme",
          "cardinality": "many"
        }
      },
      "features": [
        "include-uri"
      ],
      "new-resource-base": "http://data.lblod.info/id/concept/"
    }
  }
}
