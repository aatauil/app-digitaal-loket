    ##########################################################
    extEng:costTitleF a form:Field ;
        sh:name "Titel kost (en.)" ;
        sh:order 1 ;
        form:language "en";
        form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
        sh:path dct:title ;
        form:displayType displayTypes:defaultInput ;
        sh:group extEng:costsPg-sub .

    ##########################################################
    # Field (En.): Kost description
    ##########################################################
    extEng:costDescriptionF a form:Field ;
        sh:name "Beschrijving kost (en.)" ;
        sh:order 2 ;
        form:language "en";
        form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
