##########################################################
# form
##########################################################
extEng:form a form:Form, form:TopLevelForm ;
    form:includes extEng:titleF;
    form:includes extEng:descriptionF;
    form:includes extEng:exceptionF;
    form:includes extEng:requirementsL;
    form:includes extEng:rulesL;
    form:includes extEng:costsL;
    form:includes extEng:regulationitleF;
    form:includes extEng:websitesL.

##########################################################
#  property-group: title
##########################################################
extEng:titlePg a form:PropertyGroup;
    sh:name "Titel (en.)" ;
    sh:order 1 .

  ##########################################################
  #  field (en.): title
  ##########################################################
  extEng:titleF a form:Field ;
      sh:order 1 ;
      form:language "en";
      form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
      sh:path dct:title ;
      form:displayType displayTypes:richText ;
      sh:group extEng:titlePg .

##########################################################
#  property-group: Beschrijving
##########################################################
extEng:descriptionPg a form:PropertyGroup;
    sh:name "Beschrijving (en.)" ;
    sh:order 2 .

  ##########################################################
  #  field (en.): description
  ##########################################################
  extEng:descriptionF a form:Field ;
      sh:order 1 ;
      form:language "en";
      form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
      sh:path dct:description ;
      form:displayType displayTypes:richText ;
      sh:group extEng:descriptionPg .

##########################################################
#  property-group: exception
##########################################################
extEng:exceptionPg a form:PropertyGroup;
    sh:name "Uitzonderingen" ;
    form:help "Geef hier mogelijke uitzondering op.";
    sh:order 3 .

  ##########################################################
  #  field (en.): exception
  ##########################################################
  extEng:exceptionF a form:Field ;
      sh:order 1 ;
      form:language "en";
      form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
      sh:path lpdcExt:exception ;
      form:displayType displayTypes:richText ;
      sh:group extEng:exceptionPg .

##########################################################
#  property-group: Voorwaarden
##########################################################
extEng:requirementsPg a form:PropertyGroup;
    sh:name "Voorwaarden" ;
    form:help "Hier kan u de voorwaarden van de dienstverlening aanpassen/toevoegen.";
    sh:order 3 .

  ##########################################################
  #  listing-scope: Voorwaarden
  ##########################################################
  extEng:requirementsS a form:Scope;
    sh:path belgif:hasRequirement.

  ##########################################################
  #  Listing: Voorwaarden
  ##########################################################
  extEng:requirementsL a form:Listing;
    form:each extEng:requirementsSf;
    form:scope extEng:requirementsS;
    form:createGenerator extEng:requirementsG;
    sh:group extEng:requirementsPg;
    form:addLabel "Voeg voorwaarde toe";
    form:canAdd true;
    form:canRemove true;
    sh:maxCount 1;
    sh:order 1 .

 ##########################################################
  #  Generator: Voorwaarden
  ##########################################################
  extEng:requirementsG a form:Generator;
    form:prototype [
      form:shape [
        a m8g:Requirement ;
        dct:description " "@nl;
        dct:title " "@nl;
        dct:description " "@en;
        dct:title " "@en
      ];
    ];
    form:dataGenerator form:addMuUuid.

  ##########################################################
  #  property-group: Voorwaarden-sub
  ##########################################################
  extEng:requirementsPg-sub a form:PropertyGroup;
      sh:order 1 .

  ##########################################################
  #  Subform: Voorwaarden
  ##########################################################
  extEng:requirementsSf a form:SubForm;
     form:removeLabel "Verwijder voorwaarde";
     form:includes extEng:requirementTitleF;
     form:includes extEng:requirementDescriptionF;
     form:includes extEng:evidencesL.

    ##########################################################
    # Field (En.): Requirement title
    ##########################################################
    extEng:requirementTitleF a form:Field ;
        sh:name "Titel voorwaarde (en.)" ;
        sh:order 1 ;
        form:language "en";
        form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
        sh:path dct:title ;
        form:displayType displayTypes:defaultInput ;
        sh:group extEng:requirementsPg-sub .

    ##########################################################
    # Field (En.): Requirement description
    ##########################################################
    extEng:requirementDescriptionF a form:Field ;
        sh:name "Beschrijving voorwaarde (en.)" ;
        sh:order 2 ;
        form:language "en";
        form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
        sh:path dct:description ;
        form:displayType displayTypes:richText ;
        sh:group extEng:requirementsPg-sub .

        ##########################################################
        #  property-group: evidence
        ##########################################################
        extEng:evidencesPg a form:PropertyGroup;
            sh:name "Bewijsstukken" ;
            form:help "Van het bovenstaande voorwaarde, kan u de gevraagde bewijsstukken opgeven";
            sh:order 1 .

        ##########################################################
        #  listing-scope: Evidence
        ##########################################################
        extEng:evidencesS a form:Scope;
          sh:path m8g:hasSupportingEvidence.

        ##########################################################
        #  Listing: Evidence
        ##########################################################
        extEng:evidencesL a form:Listing;
          form:each extEng:evidencesSf;
          form:scope extEng:evidencesS;
          form:createGenerator extEng:evidencesG;
          sh:group extEng:evidencesPg;
          form:addLabel "Voeg bewijsstuk toe";
          form:canAdd true;
          form:canRemove true;
          sh:maxCount 1;
          sh:order 1 .

       ##########################################################
        #  Generator: Evidence
        ##########################################################
        extEng:evidencesG a form:Generator;
          form:prototype [
            form:shape [
              a m8g:Evidence ;
              dct:description " "@nl;
              dct:title " "@nl;
              dct:description " "@en;
              dct:title " "@en
            ];
          ];
          form:dataGenerator form:addMuUuid.

        ##########################################################
        #  property-group: Evidence-sub
        ##########################################################
        extEng:evidencesPg-sub a form:PropertyGroup;
            sh:order 1 .

        ##########################################################
        #  Subform: Evidence
        ##########################################################
        extEng:evidencesSf a form:SubForm;
           form:removeLabel "Verwijder bewijsstuk";
           form:includes extEng:evidenceTitleF;
           form:includes extEng:evidenceDescriptionF.

          ##########################################################
          # Field (En.): Evidence title
          ##########################################################
          extEng:evidenceTitleF a form:Field ;
              sh:name "Naam bewijsstuk (en.)" ;
              sh:order 1 ;
              form:language "en";
              form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
              sh:path dct:title ;
              form:displayType displayTypes:defaultInput ;
              sh:group extEng:evidencesPg-sub .

          ##########################################################
          # Field (En.): Evidence description
          ##########################################################
          extEng:evidenceDescriptionF a form:Field ;
              sh:name "Beschrijving bewijsstuk (en.)" ;
              sh:order 2 ;
              form:language "en";
              form:help "Geef in het bovenstaand veld de Engelse vertaling op.";
              sh:path dct:description ;
              form:displayType displayTypes:richText ;
              sh:group extEng:evidencesPg-sub .


