    form:canRemove false;
    sh:maxCount -1;
    sh:order 1 .

 ##########################################################
  #  Generator: Kost
  ##########################################################
  extEng:costsG a form:Generator;
    form:prototype [
      form:shape [
        a m8g:Cost;
        dct:description " "@nl;
